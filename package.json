{
  "name": "adc-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "lint:scss": "stylelint '**/src/**/*.scss' --syntax scss",
    "lint:scss:fix": "stylelint '**/src/**/*.scss' --syntax scss --fix",
    "lint:ts": "eslint . --ext .ts",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "admin:build-analyzer": "nx run admin:build --stats-json && webpack-bundle-analyzer dist/apps/admin/stats.json",
    "mobile:build-analyzer": "nx run mobile:build --stats-json && webpack-bundle-analyzer dist/apps/mobile/stats.json",
    "update:api": "openapi-generator-cli generate -i https://avc-api.azurewebsites.net/swagger/v1/swagger.yaml -g typescript-angular -o libs/shared/api/src/lib/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.0.0",
    "@angular/common": "12.0.0",
    "@angular/compiler": "12.0.0",
    "@angular/core": "12.0.0",
    "@angular/forms": "12.0.0",
    "@angular/platform-browser": "12.0.0",
    "@angular/platform-browser-dynamic": "12.0.0",
    "@angular/router": "12.0.0",
    "@capacitor/core": "^2.4.5",
    "@ionic-native/core": "^5.30.0",
    "@ionic-native/splash-screen": "^5.30.0",
    "@ionic-native/status-bar": "^5.30.0",
    "@ionic/angular": "^5.5.2",
    "@ngneat/tailwind": "^7.0.3",
    "@ngneat/transloco": "^2.21.0",
    "@ngxs/devtools-plugin": "^3.3.2",
    "@ngxs/logger-plugin": "^3.3.2",
    "@ngxs/schematics": "0.0.1-alpha.5",
    "@ngxs/storage-plugin": "^3.7.2",
    "@ngxs/store": "^3.3.2",
    "@nrwl/angular": "12.3.5",
    "@taiga-ui/cdk": "^2.10.1",
    "@taiga-ui/core": "^2.10.1",
    "@taiga-ui/icons": "^2.10.1",
    "@taiga-ui/kit": "^2.10.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.0.0",
    "@angular-eslint/eslint-plugin": "~12.0.0",
    "@angular-eslint/eslint-plugin-template": "~12.0.0",
    "@angular-eslint/template-parser": "~12.0.0",
    "@angular/cli": "12.0.0",
    "@angular/compiler-cli": "12.0.0",
    "@angular/language-service": "12.0.0",
    "@capacitor/android": "^2.4.5",
    "@capacitor/ios": "^2.4.5",
    "@ngxs/schematics": "^0.0.1-alpha.5",
    "@nrwl/cli": "12.3.5",
    "@nrwl/cypress": "12.3.5",
    "@nrwl/eslint-plugin-nx": "12.3.5",
    "@nrwl/jest": "12.3.5",
    "@nrwl/linter": "12.3.5",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "12.3.5",
    "@nrwl/workspace": "12.3.5",
    "@nxtend/capacitor": "^11.0.0",
    "@nxtend/ionic-angular": "^11.1.1",
    "@openapitools/openapi-generator-cli": "^2.3.3",
    "@tailwindcss/aspect-ratio": "0.2.1",
    "@tailwindcss/line-clamp": "0.2.1",
    "@types/jest": "26.0.8",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "cypress": "^7.3.0",
    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "^6.0.0",
    "jest": "26.2.2",
    "jest-preset-angular": "8.4.0",
    "lint-staged": "^11.0.0",
    "ng-packagr": "^12.0.0",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "tailwindcss": "2.1.2",
    "ts-jest": "26.5.5",
    "ts-node": "~9.1.1",
    "typescript": "~4.2.4",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint . --fix",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax scss --fix",
      "git add"
    ]
  }
}
